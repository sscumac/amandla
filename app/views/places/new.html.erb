<div class="form container">
  <h2>Add your place</h2>
      <%= simple_form_for [@place] do |f| %>
        <%= f.input :name %>
        <%= f.input :category, as: :radio_buttons, collection: Place.all_cats %>
        <%= f.input :address %>
        <%= f.input :story %>
        <%= f.input :tag_list, as: :check_boxes, collection: Place.all_tags %> 
        <%= f.input :photo, as: :file %>
        <%= f.submit "Submit me",  class:"btn-main" %>
      <% end %>
    </div>
  </div>

</div>